    "#Content Rating     object\n",
    "#Genres             object\n",
    "#Last Updated       object\n",
    "#Current Ver        object\n",
    "#Android Ver        object\n",
    "\n",
    "X = pd.DataFrame(X)\n",
    "X = X.convert_objects(convert_numeric=True)\n",
    "#X.columns = onehotlabels.T[:] #['Category', 'Rating', 'Reviews', 'Size', 'Installs', 'Type','Content Rating']\n",
    "\n",
    "# Eliminating null values\n",
    "#for i in X.columns:\n",
